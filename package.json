{
  "name": "upload",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch src/api/server.ts",
    "build": "tsc -b && vite build",
    "start": "concurrently -n web,api -c auto \"vite\" \"tsx watch src/api/server.ts\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "prettier": "prettier --check '{api,src}/**/*{js,jsx,ts,tsx,md,html,css,scss}'",
    "prettier:write": "prettier --write '{api,src}/**/*{js,jsx,ts,tsx,md,html,css,scss}'"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/cors": "2.8.19",
    "@types/multer": "2.0.0",
    "@types/node": "25.1.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "concurrently": "9.2.1",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "17.2.0",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "cors": "2.8.6",
    "express": "5.2.1",
    "multer": "2.0.2",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  }
}
